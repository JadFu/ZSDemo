<!-- app/views/users/login.html.erb -->
<h1>Login</h1>

<%= form_with(model: @user, url: login_path) do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email, required: true %>

  <%= form.label :password %>
  <%= form.password_field :password, required: true %>

  <%= form.submit 'Login' %>
<% end %>